<div class="row justify-content-center mt-5">
  <% for (let i = 0; i < categories.length; i++) { %>
  <figure class="category" onclick="ajax('/<%= categories[i].link %>')">
    <img class="cat-img" src="/static/<%= categories[i].image %>.png" alt="<%= categories[i].image %>" />
    <figcaption><%= categories[i].title %></figcaption>
  </figure>
  <% } %>
</div>
